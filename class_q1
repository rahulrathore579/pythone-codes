class Rectangle:
    def _init_(self,length,breath):
        self.length=length
        self.breath=breath
        
    def area(self):
        print(f'area of reactangle = {self.length * self.breath}')
        
    def perimeter(self):
        print(f'parameret is ={2*self.length + 2*self.breath}')

r1= Rectangle(10,20)
r1.area()
r1.perimeter() r2=Rectangle(23,5)
r2.area()
r2.perimeter()
